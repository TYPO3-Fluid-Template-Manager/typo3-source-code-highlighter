<T3DataStructure>
  <meta>
    <langDisable>1</langDisable> 
  </meta>
  <sheets>
  <sVIEW>
  <ROOT>
    <TCEforms>
	<sheetTitle>LLL:EXT:sema_sourcecode/pi1/locallang.php:contentsheet</sheetTitle>
	</TCEforms>
    <type>array</type>
    <el>
	<cLang>
		<TCEforms>
			<label>LLL:EXT:sema_sourcecode/pi1/locallang.php:lang</label>
			<config>
          <type>select</type>
          <itemsProcFunc>tx_sema_sourcecode_addFieldsToFlexForm->addFields</itemsProcFunc>
          </config>
		</TCEforms>
	</cLang>
	
	<cCode>
		<TCEforms>
			<label>LLL:EXT:sema_sourcecode/pi1/locallang.php:code</label>
			<config>
				<type>text</type>
				<cols>120</cols>
				<rows>12</rows>
			</config>
		</TCEforms>
	</cCode>
	</el>
	</ROOT>
	</sVIEW>
    <sOPTIONS>
		<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:sema_sourcecode/pi1/locallang.php:optionsheet</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
		<cStartnumber>
			<TCEforms>
				<label>LLL:EXT:sema_sourcecode/pi1/locallang.php:startnumber</label>
				<config>
					<type>input</type>
					<eval>int</eval>
					<size>4</size>
					<default>1</default>
				</config>
			</TCEforms>
		</cStartnumber>
		<cHighlight>
		<TCEforms>
			<label>LLL:EXT:sema_sourcecode/pi1/locallang.php:highlight</label>
			<config>
				<type>input</type>
				<eval>tx_semasourcecode_evalhighlight</eval>
			</config>
		</TCEforms>
		</cHighlight>
		</el>
		</ROOT>
	</sOPTIONS>
    </sheets>
</T3DataStructure>
